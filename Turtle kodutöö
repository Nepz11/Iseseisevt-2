import turtle
import random

def majad():
    
    turtle.speed(10)
    turtle.pensize(5)
    turtle.penup()
    turtle.goto(-500,0)
    turtle.pendown()

    turtle.color("navyblue")
    turtle.fd(50)
    turtle.rt(90)
    turtle.bk(70)
    turtle.rt(90)
    turtle.fd(50)
    turtle.rt(90)
    turtle.bk(70)

    turtle.goto(-530,0)
    turtle.color("black")
    turtle.fd(100)
    turtle.rt(90)
    for i in range(3):
        turtle.color("sea green")
        turtle.forward(110)
        turtle.lt(120)
    turtle.color("black")
    turtle.fd(110)
    turtle.rt(90)
    turtle.fd(100)
    turtle.rt(90)
    turtle.fd(110)
    turtle.lt(180)

    turtle.penup()
    turtle.goto(-350,0)
    turtle.pendown()

    turtle.color("navyblue")
    turtle.fd(50)
    turtle.rt(90)
    turtle.bk(70)
    turtle.rt(90)
    turtle.fd(50)
    turtle.rt(90)
    turtle.bk(70)

